          /* istanbul ignore next */
          (cov_1g9zbwmm0d().s[78]++, content2.split('\n'));
          const maxLines =
          /* istanbul ignore next */
          (cov_1g9zbwmm0d().s[79]++, Math.max(lines1.length, lines2.length));
          const differences =
          /* istanbul ignore next */
