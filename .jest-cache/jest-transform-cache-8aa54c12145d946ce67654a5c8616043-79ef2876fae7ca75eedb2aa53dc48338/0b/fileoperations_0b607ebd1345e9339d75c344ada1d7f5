          /* istanbul ignore next */
          cov_1g9zbwmm0d().s[105]++;
          fileInfo.hash = {
            algorithm: args.hashAlgorithm,
            value: hash
          };
        } else
        /* istanbul ignore next */
        {
          cov_1g9zbwmm0d().b[24][1]++;
        }
        cov_1g9zbwmm0d().s[106]++;
