  const textSample =
  /* istanbul ignore next */
  (cov_1g9zbwmm0d().s[69]++, buffer.subarray(0, 100));
  const isText =
  /* istanbul ignore next */
  (cov_1g9zbwmm0d().s[70]++, textSample.every(byte => {
    /* istanbul ignore next */
