      /* istanbul ignore next */
      cov_1g9zbwmm0d().b[22][0]++;
      cov_1g9zbwmm0d().s[68]++;
      return mimeType;
    } else
    /* istanbul ignore next */
    {
      cov_1g9zbwmm0d().b[22][1]++;
    }
  }
