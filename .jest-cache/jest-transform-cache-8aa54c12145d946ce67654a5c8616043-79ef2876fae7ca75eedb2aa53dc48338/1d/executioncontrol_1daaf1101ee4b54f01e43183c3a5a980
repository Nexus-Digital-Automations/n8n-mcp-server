          cov_20mszqtp72().s[84]++;
          stateManager.initializeExecution(execution);
          /* istanbul ignore next */
          cov_20mszqtp72().s[85]++;
          context = stateManager.getExecutionContext(args.executionId);
        } else
        /* istanbul ignore next */
        {
          cov_20mszqtp72().b[33][1]++;
        }
        // Validate target nodes exist
