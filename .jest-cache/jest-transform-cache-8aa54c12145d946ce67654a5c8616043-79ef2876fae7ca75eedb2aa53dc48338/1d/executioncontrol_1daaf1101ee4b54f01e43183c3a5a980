    annotations: {
      title: 'Monitor Executions',
      readOnlyHint: true,
      destructiveHint: false,
      idempotentHint: true,
      openWorldHint: true
    },
    execute: async args => {
      /* istanbul ignore next */
      cov_20mszqtp72().f[18]++;
      cov_20mszqtp72().s[220]++;
      try {
        const client =
        /* istanbul ignore next */
        (cov_20mszqtp72().s[221]++, getClient());
