    /* istanbul ignore next */
    cov_2jz21e4ydh().f[1]++;
    cov_2jz21e4ydh().s[6]++;
    this.n8nClient = client;
  }
  /**
   * Register OAuth2 provider configuration
   */
  registerProvider(config) {
    /* istanbul ignore next */
